{
  "name": "tdd-demo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./public/*",
    "localhost": "sleep 1; which open && open http://localhost:3000",
    "browser": "sleep 2; concurrent 'npm run start' 'npm run localhost'",
    "webpack": "webpack --verbose --colors --display-eror-details --config ./webpack/production.config.js",
    "webpack:watch": "NODE_ENV=development node --harmony ./webpack/dev-server.js",
    "prebuild": "npm run clean -s",
    "build": "npm run test -s && npm run webpack -s",
    "serve": "NODE_ENV=production NODE_PATH=\"./app\" supervisor --harmony -e js ./index.js",
    "serve:development": "NODE_ENV=development NODE_PATH=\"./app\" supervisor --harmony -e js ./index.js",
    "watch": "npm run clean -s && concurrent --kill-others 'npm run webpack:watch -s' 'npm run serve:development -s' 'npm run test:watch'",
    "start": "NODE_ENV=production npm run build -s && npm run serve -s",
    "test": "mocha --harmony --colors --compilers js:babel/register ./**/__tests__/**/*Test.js",
    "test:watch": "npm run test -- -w"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel": "^5.4.7",
    "babel-eslint": "^3.1.7",
    "babel-runtime": "^5.4.7",
    "concurrently": "0.0.5",
    "koa": "^0.20.0",
    "koa-compress": "^1.0.8",
    "koa-mount": "^1.3.0",
    "koa-router": "^5.0.1",
    "koa-session": "^3.1.0",
    "pg": "^4.3.0",
    "pg-hstore": "^2.3.2",
    "react": "^0.13.3",
    "react-router": "^0.13.3",
    "sequelize": "^3.1.0",
    "sequelize-cli": "^1.7.0",
    "supervisor": "^0.6.0"
  },
  "devDependencies": {
    "babel-core": "^5.4.7",
    "babel-loader": "^5.1.3",
    "chai": "^2.3.0",
    "css-loader": "^0.13.1",
    "eslint": "^0.21.2",
    "eslint-loader": "^0.11.2",
    "extract-text-webpack-plugin": "^0.8.0",
    "jeet": "^6.1.2",
    "json-loader": "^0.5.1",
    "mocha": "^2.2.5",
    "nib": "^1.1.0",
    "node-jsdom": "^3.1.5",
    "node-libs-browser": "^0.5.2",
    "react-hot-loader": "^1.2.7",
    "rimraf": "^2.3.4",
    "rupture": "^0.6.1",
    "sinon": "^1.14.1",
    "sinon-chai": "^2.7.0",
    "strip-loader": "^0.1.0",
    "style-loader": "^0.12.2",
    "stylus-loader": "^1.1.1",
    "supertest": "^1.0.1",
    "webpack": "^1.9.8",
    "webpack-dev-server": "^1.9.0"
  }
}
